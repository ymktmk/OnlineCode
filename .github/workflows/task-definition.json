{
      "containerDefinitions": [
            {
                  "name": "code",
                  "image": "",
                  "essential": true,
                  "command": ["/main"],
                  "portMappings" : [
                        {
                              "containerPort": 10000,
                              "hostPort": 10000,
                              "protocol": "tcp"
                        }
                  ],
                  "mountPoints" : [
                        {
                              "containerPath" : "/go/src/work",
                              "readOnly"      : false,
                              "sourceVolume"  : "mount"
                        },
                        {
                              "containerPath" : "/var/run/docker.sock",
                              "sourceVolume"  : "socket"
                        }
                  ],
                  "logConfiguration" : {
                        "logDriver" : "awslogs",
                        "options"   : {
                              "awslogs-region"        : "ap-northeast-1",
                              "awslogs-stream-prefix" : "/aws/ecs",
                              "awslogs-group"         : "online-code"
                        }
                  }
            }
      ],
      "runtimePlatform": {
            "cpuArchitecture": "X86_64",
            "operatingSystemFamily": "LINUX"
      },
      "executionRoleArn": "ecsTaskExecutionRole",
      "family": "online-code",
      "cpu": "256",
      "memory": "512",
      "requiresCompatibilities": ["EC2"],
      "volumes": [
            {
                  "name": "mount",
                  "host": {
                        "sourcePath": "/"
                  }
            },
            {
                  "name": "socket",
                  "host": {
                        "sourcePath": "/var/run/docker.sock"
                  }
            }
      ]
}